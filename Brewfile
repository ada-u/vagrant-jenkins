# Update homebrew
update

# Upgrade already-installed packages
upgrade

# Add repositories
tap homebrew/binary
tap homebrew/versions
tap phinze/cask

# Install brew-cask
tap phinze/homebrew-cask
install brew-cask

# Install brew-packages
install ansible
install brew-cask

# Install cask-packages
cask install virtualbox
cask install vagrant

# Remove outdated version
cleanup
